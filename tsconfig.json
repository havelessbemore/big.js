{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noLib": false,
    "outDir": "./lib",
    "preserveConstEnums": false,
    "removeComments": true,
    "sourceMap": true,
    "stripInternal": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**",
    "!lib/**",
    "!dist/**",
    "!.git/**"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "src/add/basicAdditionMethod.ts",
    "src/add/reverseAdditionMethod.ts",
    "src/bigInt.ts",
    "src/div/basicDivisionMethod.ts",
    "src/div/basicHalfMethod.ts",
    "src/div/simpleDivisionMethod.ts",
    "src/index.ts",
    "src/mul/basicDoubleMethod.ts",
    "src/mul/basicMultiplicationMethod.ts",
    "src/mul/basicSquareMethod.ts",
    "src/mul/karatsubaMultiplicationMethod.ts",
    "src/mul/karatsubaSquareMethod.ts",
    "src/sub/basicSubtractionMethod.ts",
    "src/sub/reverseSubtractionMethod.ts",
    "src/util.ts",
    "test/add/basicAdditionMethod.ts",
    "test/add/reverseAdditionMethod.ts",
    "test/div/basicDivisionMethod.ts",
    "test/div/basicHalfMethod.ts",
    "test/mul/basicDoubleMethod.ts",
    "test/mul/basicMultiplicationMethod.ts",
    "test/mul/basicSquareMethod.ts",
    "test/mul/karatsubaMultiplicationMethod.ts",
    "test/mul/karatsubaSquareMethod.ts",
    "test/sub/basicSubtractionMethod.ts",
    "test/sub/reverseSubtractionMethod.ts",
    "typings/globals/mocha/index.d.ts",
    "typings/globals/node/index.d.ts",
    "typings/index.d.ts"
  ]
}
