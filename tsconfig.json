{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noLib": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "./lib",
    "preserveConstEnums": false,
    "removeComments": true,
    "sourceMap": true,
    "stripInternal": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es6"
  },
  "filesGlob": [
    "**/*.ts",
    "**/*.tsx",
    "!node_modules/**",
    "!lib/**",
    "!dist/**",
    "!.git/**"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "src/adapter/addition.ts",
    "src/adapter/decrement.ts",
    "src/adapter/increment.ts",
    "src/adapter/reverseAddition.ts",
    "src/algorithm/addition.ts",
    "src/algorithm/basicDivision.ts",
    "src/algorithm/basicMultiplication.ts",
    "src/algorithm/basicSquare.ts",
    "src/algorithm/burnikelZieglerDivision.ts",
    "src/algorithm/decrement.ts",
    "src/algorithm/double.ts",
    "src/algorithm/halve.ts",
    "src/algorithm/increment.ts",
    "src/algorithm/karatsubaMultiplication.ts",
    "src/algorithm/karatsubaSquare.ts",
    "src/algorithm/reverseAddition.ts",
    "src/algorithm/reverseSubtraction.ts",
    "src/algorithm/singleDigitDivision.ts",
    "src/algorithm/singleDigitMultiplication.ts",
    "src/algorithm/subtraction.ts",
    "src/bigMint.ts",
    "src/index.ts",
    "src/type/binaryOperation.ts",
    "src/type/integer.ts",
    "src/type/real.ts",
    "src/type/unaryOperation.ts",
    "src/util/arrayUtils.ts",
    "src/util/numUtils.ts",
    "test/algorithm/addition.ts",
    "test/algorithm/basicDivision.ts",
    "test/algorithm/basicMultiplication.ts",
    "test/algorithm/basicSquare.ts",
    "test/algorithm/burnikelZieglerDivision.ts",
    "test/algorithm/double.ts",
    "test/algorithm/halve.ts",
    "test/algorithm/karatsubaMultiplication.ts",
    "test/algorithm/karatsubaSquare.ts",
    "test/algorithm/reverseAddition.ts",
    "test/algorithm/reverseSubtraction.ts",
    "test/algorithm/singleDigitDivision.ts",
    "test/algorithm/subtraction.ts",
    "test/bigMint.ts",
    "typings/globals/chai/index.d.ts",
    "typings/globals/mocha/index.d.ts",
    "typings/globals/node/index.d.ts",
    "typings/globals/sinon/index.d.ts",
    "typings/index.d.ts"
  ]
}
